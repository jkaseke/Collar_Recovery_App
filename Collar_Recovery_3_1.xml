      <bind esri:fieldType="null" nodeset="/Collar_Recovery_3_1/collar_info/barcode_prev" readonly="true()" relevant=" /Collar_Recovery_3_1/collar_recovered  = 'yes' or  /Collar_Recovery_3_1/animal_death  = 'no'" type="barcode"/>
      <bind calculate="if(substr( /Collar_Recovery_3_1/collar_info/barcode_prev ,0,3)=&quot;ATS&quot;,&quot;ATS&quot;,&quot;&quot;)" nodeset="/Collar_Recovery_3_1/collar_info/prev_manufacturer" relevant=" /Collar_Recovery_3_1/collar_recovered  = 'yes' or  /Collar_Recovery_3_1/animal_death  = 'no' or  /Collar_Recovery_3_1/collar_recovered  = 'no'" required="true()" type="select1"/>
      <bind calculate="substr( /Collar_Recovery_3_1/collar_info/barcode_prev ,12,18)" esri:fieldLength="6" nodeset="/Collar_Recovery_3_1/collar_info/prev_collar_id" relevant=" /Collar_Recovery_3_1/collar_recovered  = 'yes' or  /Collar_Recovery_3_1/animal_death  = 'no' or  /Collar_Recovery_3_1/collar_recovered  = 'no'" required="true()" type="string"/>
      <bind calculate="substr( /Collar_Recovery_3_1/collar_info/barcode_prev ,24,31)" esri:fieldLength="7" nodeset="/Collar_Recovery_3_1/collar_info/prev_freq" relevant=" /Collar_Recovery_3_1/collar_recovered  = 'yes' or  /Collar_Recovery_3_1/animal_death  = 'no' or  /Collar_Recovery_3_1/collar_recovered  = 'no'" required="true()" type="decimal"/>
      <bind nodeset="/Collar_Recovery_3_1/collar_info/collar_photo" relevant=" /Collar_Recovery_3_1/collar_recovered  = 'yes' or  /Collar_Recovery_3_1/animal_death  = 'no'" type="binary"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info" relevant=" /Collar_Recovery_3_1/animal_death  = 'yes'"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/mort_date_estimated" required="true()" type="date"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/carcass_intact" required="true()" type="select1"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/mortality_cause" required="true()" type="select1"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/cause_other" relevant=" /Collar_Recovery_3_1/animal_info/mortality_cause  = 'cause_other'" required="true()" type="string"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/mortality_subcause" relevant=" /Collar_Recovery_3_1/animal_info/mortality_cause  = 'Accident' or  /Collar_Recovery_3_1/animal_info/mortality_cause  = 'Predation' or  /Collar_Recovery_3_1/animal_info/mortality_cause  = 'Unknown'" required="true()" type="select1"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/mortality_other" relevant=" /Collar_Recovery_3_1/animal_info/mortality_subcause  = 'accident_other' or  /Collar_Recovery_3_1/animal_info/mortality_subcause  = 'predation_other' or  /Collar_Recovery_3_1/animal_info/mortality_subcause  = 'unknown_other'" required="true()" type="string"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/body_condition" type="select1"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/marrow_condition" relevant=" /Collar_Recovery_3_1/animal_info/body_condition  = 'yes'" type="select1"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/marrow_photo" relevant=" /Collar_Recovery_3_1/animal_info/body_condition  = 'yes'" required="true()" type="binary"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/rump_fat_depth" relevant=" /Collar_Recovery_3_1/animal_info/body_condition  = 'yes'" type="decimal"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/pulling_tooth" readonly="true()" type="string"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/tooth_removed" type="select1"/>
      <bind calculate=" /Collar_Recovery_3_1/collar_info/prev_collar_id " nodeset="/Collar_Recovery_3_1/animal_info/sample_number" relevant=" /Collar_Recovery_3_1/animal_info/tooth_removed  = 'yes'" required="true()" type="string"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/animal_photo1" type="binary"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/animal_photo2" type="binary"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/animal_photo3" type="binary"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/animal_photo4" type="binary"/>
      <bind nodeset="/Collar_Recovery_3_1/animal_info/veg_photo" type="binary"/>
      <bind nodeset="/Collar_Recovery_3_1/capture_location" type="geopoint"/>
      <bind calculate="pulldata('@geopoint',  /Collar_Recovery_3_1/capture_location , 'y')" nodeset="/Collar_Recovery_3_1/capture_lat" type="decimal"/>
      <bind calculate="pulldata('@geopoint',  /Collar_Recovery_3_1/capture_location , 'x')" nodeset="/Collar_Recovery_3_1/capture_long" type="decimal"/>
      <bind nodeset="/Collar_Recovery_3_1/notes_general/notes" type="string"/>
      <bind nodeset="/Collar_Recovery_3_1/magnet_reminder" readonly="true()" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/Collar_Recovery_3_1/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/Collar_Recovery_3_1/collector_firstname">
      <label>First Name</label>
    </input>
    <input ref="/Collar_Recovery_3_1/collector_lastname">
      <label>Last Name</label>
    </input>
    <input ref="/Collar_Recovery_3_1/collector_email">
      <label>Email Address</label>
    </input>
    <input ref="/Collar_Recovery_3_1/date">
      <label>Data Collection Date/Time</label>
    </input>
    <select1 ref="/Collar_Recovery_3_1/affiliation_org">
      <label>Affiliation</label>
      <item>
        <label>UDWR</label>
        <value>UDWR</value>
      </item>
      <item>
        <label>BYU</label>
        <value>BYU</value>
      </item>
      <item>
        <label>USU</label>
        <value>USU</value>
      </item>
      <item>
        <label>Other</label>
        <value>Affiliation_Other</value>
      </item>
    </select1>
    <input ref="/Collar_Recovery_3_1/affiliation_other">
      <label>Other</label>
    </input>
    <select1 ref="/Collar_Recovery_3_1/collar_recovered">
      <label>Was the collar recovered?</label>
      <item>
        <label>Yes</label>
        <value>yes</value>
      </item>
      <item>
        <label>No</label>
        <value>no</value>
      </item>
    </select1>
    <select1 appearance="minimal" ref="/Collar_Recovery_3_1/species_type">
      <label>Species</label>
      <item>
        <label>Bison</label>
        <value>Bison</value>
      </item>
      <item>
        <label>Black Bear</label>
        <value>Black_Bear</value>
      </item>
      <item>
        <label>Bobcat</label>
        <value>Bobcat</value>
      </item>
      <item>
        <label>California Bighorn</label>
        <value>California_Bighorn</value>
      </item>
      <item>
        <label>Cougar</label>
        <value>Cougar</value>
      </item>
      <item>
        <label>Desert Bighorn</label>
        <value>Desert_Bighorn</value>
      </item>
      <item>
        <label>Elk</label>
        <value>Elk</value>
      </item>
      <item>
        <label>Moose</label>
        <value>Moose</value>
      </item>
      <item>
        <label>Mountain Goat</label>
        <value>Mountain_Goat</value>
      </item>
      <item>
        <label>Mule Deer</label>
        <value>Mule_Deer</value>
      </item>
      <item>
        <label>Pronghorn</label>
        <value>Pronghorn</value>
      </item>
      <item>
        <label>Rocky Mountain Bighorn</label>
        <value>Rocky_Mountain_Bighorn</value>
      </item>
      <item>
        <label>Turkey</label>
        <value>Turkey</value>
      </item>
    </select1>
    <select1 ref="/Collar_Recovery_3_1/not_recovered">
      <label>Why was the collar not recovered?</label>
      <item>
        <label>Inaccessible terrain</label>
        <value>terrain</value>
      </item>
      <item>
        <label>Collar quit transmitting</label>
        <value>no_signal</value>
      </item>
      <item>
        <label>Animal likely harvested but collar was not returned</label>
        <value>hunted</value>
      </item>
      <item>
        <label>Other</label>
        <value>other_recover</value>
      </item>
    </select1>
    <input ref="/Collar_Recovery_3_1/why_notrecovered">
      <label>Other</label>
    </input>
    <select1 ref="/Collar_Recovery_3_1/animal_death">
      <label>Did the animal die?</label>
      <item>
        <label>Yes</label>
        <value>yes</value>
      </item>
      <item>
        <label>No</label>
        <value>no</value>
      </item>
    </select1>
    <select1 ref="/Collar_Recovery_3_1/collar_fall">
      <label>Why did the collar fall off?</label>
      <item>
        <label>Collar band worn or damaged</label>
        <value>worn_damaged</value>
      </item>
      <item>
        <label>Animal slipped collar off</label>
        <value>slipped_off</value>
      </item>
      <item>
        <label>Drop-off mechanism activated</label>
        <value>dropped_off</value>
      </item>
      <item>
        <label>Other</label>
        <value>other_none</value>
      </item>
    </select1>
    <input ref="/Collar_Recovery_3_1/why_nocollar">
      <label>Other</label>
    </input>
    <group ref="/Collar_Recovery_3_1/collar_info">
      <label>Collar Information</label>
      <input appearance="minimal" ref="/Collar_Recovery_3_1/collar_info/barcode_prev">
        <label>QR Code</label>
      </input>
      <select1 ref="/Collar_Recovery_3_1/collar_info/prev_manufacturer">
        <label>Manufacturer</label>
        <item>
          <label>ATS</label>
          <value>ATS</value>
        </item>
        <item>
          <label>Lotek</label>
          <value>Lotek</value>
        </item>
        <item>
          <label>Microwave</label>
          <value>Microwave</value>
        </item>
      </select1>
      <input ref="/Collar_Recovery_3_1/collar_info/prev_collar_id">
        <label>Serial Number (6 digits, e.g., 041911)</label>
      </input>
      <input ref="/Collar_Recovery_3_1/collar_info/prev_freq">
        <label>Frequency (6 digits, e.g., 149.200)</label>
      </input>
      <upload mediatype="image/*" ref="/Collar_Recovery_3_1/collar_info/collar_photo">
        <label>Label Photo</label>
      </upload>
    </group>
    <group ref="/Collar_Recovery_3_1/animal_info">
      <label>Animal Mortality Information</label>
      <input ref="/Collar_Recovery_3_1/animal_info/mort_date_estimated">
        <label>Estimated Mortality Date</label>
      </input>
      <select1 ref="/Collar_Recovery_3_1/animal_info/carcass_intact">
        <label>Carcass Intact?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select1 appearance="autocomplete" ref="/Collar_Recovery_3_1/animal_info/mortality_cause">
        <label>Mortality Cause</label>
        <item>
          <label>Accident</label>
          <value>Accident</value>
        </item>
        <item>
          <label>Predation</label>
          <value>Predation</value>
        </item>
        <item>
          <label>Malnutrition</label>
          <value>Malnutrition</value>
        </item>
        <item>
          <label>Disease</label>
          <value>Disease</value>
        </item>
        <item>
          <label>Hunting</label>
          <value>Hunting</value>
        </item>
        <item>
          <label>Removal</label>
          <value>Removal</value>
        </item>
        <item>
          <label>Poaching</label>
          <value>Poaching</value>
        </item>
        <item>
          <label>Unknown</label>
          <value>Unknown</value>
        </item>
        <item>
          <label>Other</label>
          <value>cause_other</value>
        </item>
      </select1>
      <input ref="/Collar_Recovery_3_1/animal_info/cause_other">
        <label>Other</label>
      </input>
      <select1 appearance="autocomplete" ref="/Collar_Recovery_3_1/animal_info/mortality_subcause">
        <label>Mortality Subcause</label>
        <itemset nodeset="instance('mort_subcause')/root/item[cause_filter =  /Collar_Recovery_3_1/animal_info/mortality_cause ]">
          <value ref="name"/>
          <label ref="jr:itext(itextId)"/>
        </itemset>
      </select1>
      <input ref="/Collar_Recovery_3_1/animal_info/mortality_other">
        <label>Other</label>
      </input>
      <select1 appearance="horizontal-compact" ref="/Collar_Recovery_3_1/animal_info/body_condition">
        <label>Body Condition evaluated?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <select1 ref="/Collar_Recovery_3_1/animal_info/marrow_condition">
        <label>Marrow Condition</label>
        <item>
          <label ref="jr:itext('/Collar_Recovery_3_1/animal_info/marrow_condition/1:label')"/>
          <value>1</value>
        </item>
        <item>
          <label ref="jr:itext('/Collar_Recovery_3_1/animal_info/marrow_condition/2:label')"/>
          <value>2</value>
        </item>
        <item>
          <label ref="jr:itext('/Collar_Recovery_3_1/animal_info/marrow_condition/3:label')"/>
          <value>3</value>
        </item>
        <item>
          <label ref="jr:itext('/Collar_Recovery_3_1/animal_info/marrow_condition/4:label')"/>
          <value>4</value>
        </item>
        <item>
          <label ref="jr:itext('/Collar_Recovery_3_1/animal_info/marrow_condition/5:label')"/>
          <value>5</value>
        </item>
      </select1>
      <upload mediatype="image/*" ref="/Collar_Recovery_3_1/animal_info/marrow_photo">
        <label>Marrow Photo</label>
      </upload>
      <input ref="/Collar_Recovery_3_1/animal_info/rump_fat_depth">
        <label>Rump Fat Depth (mm)</label>
      </input>
      <input ref="/Collar_Recovery_3_1/animal_info/pulling_tooth">
        <label ref="jr:itext('/Collar_Recovery_3_1/animal_info/pulling_tooth:label')"/>
      </input>
      <select1 ref="/Collar_Recovery_3_1/animal_info/tooth_removed">
        <label>Tooth Removed?</label>
        <item>
          <label>Yes</label>
          <value>yes</value>
        </item>
        <item>
          <label>No</label>
          <value>no</value>
        </item>
      </select1>
      <input ref="/Collar_Recovery_3_1/animal_info/sample_number">
        <label>Sample #</label>
      </input>
      <upload mediatype="image/*" ref="/Collar_Recovery_3_1/animal_info/animal_photo1">
        <label>Animal Photos</label>
      </upload>
      <upload mediatype="image/*" ref="/Collar_Recovery_3_1/animal_info/animal_photo2">
        <label>add.</label>
      </upload>
      <upload mediatype="image/*" ref="/Collar_Recovery_3_1/animal_info/animal_photo3">
        <label>add.</label>
      </upload>
      <upload mediatype="image/*" ref="/Collar_Recovery_3_1/animal_info/animal_photo4">
        <label>add.</label>
      </upload>
      <upload mediatype="image/*" ref="/Collar_Recovery_3_1/animal_info/veg_photo">
        <label>Vegetation Photo</label>
      </upload>
    </group>
    <input ref="/Collar_Recovery_3_1/capture_location">
      <label>Location</label>
    </input>
    <input ref="/Collar_Recovery_3_1/capture_lat">
      <label>Latitude</label>
    </input>
    <input ref="/Collar_Recovery_3_1/capture_long">
      <label>Longitude</label>
    </input>
    <group ref="/Collar_Recovery_3_1/notes_general">
      <label>Notes</label>
      <input ref="/Collar_Recovery_3_1/notes_general/notes">
        <label>Notes</label>
      </input>
    </group>
    <input ref="/Collar_Recovery_3_1/magnet_reminder">
      <label ref="jr:itext('/Collar_Recovery_3_1/magnet_reminder:label')"/>
    </input>
  </h:body>
</h:html>
